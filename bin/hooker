#!/usr/bin/env node
'use strict'

var app = require('../');

if (process.argv[2] === 'start') {
  var port = process.argv[3] || 10010
  app.listen(port, function() {
  	console.log('%s listening at %s', app.name, app.url);
  })
} else {
	var dirHash = process.argv[2];
  	console.info(app.hashCwd(dirHash));
}